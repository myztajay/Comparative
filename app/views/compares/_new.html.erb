  <div class="row  center-align">
   <div class="col s12 m4 offset-m4">
    <div class="card yellow lighten-5 z-depth-4">
      <div class="card-content black-text devise">
        <span class="card-title"><h3 class="devise">New Comparative</h3></span>
        <%= form_for @compare do |f| %>

        <%= f.label :headline%>
        <%= f.text_field :headline%>

        <%= f.label :compareimg1%>
        <%= f.file_field :compareimg1 %>
        <br>
        <%= f.label :compareimg2%>
        <%= f.file_field :compareimg2 %>

        <%= f.submit :class =>"waves-effect waves-light btn cyan accent-4"%>
        <%end%>
      </div>
    </div>
  </div>
</div>


