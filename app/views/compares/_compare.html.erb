    <div class="card yellow lighten-5 z-depth-4">
            <div class="card-content white-text">
              <span class="card-title"><h3><%= link_to @compare.headline, compare_path(@compare), :class => " cyan-text"  %></h3></span>
    <div class="compare">
      <div class="comp">
        <img  src='<%= @compare.img1_url%>'><br>
            <%= link_to "this", votes_create_path(:choice => "this", :compare_id => @compare.id.to_i), :class =>"waves-effect waves-light btn cyan accent-4" %>
            <h4 class="center-align cyan-text"><%= @compare.img1_pts %></h4>
      </div>
      <div class="comp">
        <img src='<%= @compare.img2_url%>'><br>
            <%= link_to "that", votes_create_path(:choice => "that", :compare_id => @compare.id.to_i), :class =>"waves-effect waves-light btn cyan accent-4" %>
             <h4 class="center-align cyan-text"><%= @compare.img2_pts %></h4>
      </div>
    </div> 
   
        <div class="comparesContainer flexSpaceBetween">
        <% if @compare.user_id === current_user.id %>
            <%= link_to "Edit this Comparative", edit_compare_path(@compare) %>
            <%= link_to "Delete this Comparative", compare_path(@compare), method: :delete, data: {confirm: "Are you sure?"}%>
        <% end %>
        </div>
       </div>
           
          </div>